    <deploymentStatus>Deployed</deploymentStatus>
    <description>Object stores the metadata information about the file, which was uploaded</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Captures the Account to which the file was linked</description>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Content Metadata</relationshipLabel>
        <relationshipName>Content_Metadata</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Content_Document_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Stores the ContentDocumentId of the file, which was uploaded</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>true</externalId>
        <label>Content Document Id</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Content_Size__c</fullName>
        <description>Records the content size of the document</description>
        <externalId>false</externalId>
        <label>Size</label>
        <precision>9</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Document_Created_By_User__c</fullName>
        <externalId>false</externalId>
        <formula>CreatedBy.FirstName + &apos; &apos; + CreatedBy.LastName</formula>
        <label>Document Created By User</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Document_Created_By__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Person who created the document</description>
        <externalId>false</externalId>
        <label>Document Created By</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Content_Metadata</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Document_Created_Date__c</fullName>
        <description>Created date of the document</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Document Created Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Document_Type__c</fullName>
        <description>This field tracks the document type to classify the documents.</description>
        <externalId>false</externalId>
        <label>Document Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Empf</fullName>
                    <default>false</default>
                    <label>Empf</label>
                </value>
                <value>
                    <fullName>VO</fullName>
                    <default>false</default>
                    <label>VO</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>File_Extension__c</fullName>
        <description>Stores the file extension of the file, which was uploaded</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>File Extension</label>
        <length>40</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>File_Type__c</fullName>
        <description>Stores the file type, which was uploaded</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>File Type</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Latest_Content_Version_Id__c</fullName>
        <description>Latest content version for the document.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Latest Content Version Id</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Linked_Objects__c</fullName>
        <description>Field displays the Objects a particular document is linked</description>
        <externalId>false</externalId>
        <formula>IF(AND (Account__c != NULL, Opportunity__c = NULL, Order__c = NULL), &apos;Account&apos;, 
IF(AND (Account__c != NULL, Opportunity__c != NULL, Order__c = NULL), &apos;Account, Opportunity&apos;, 
IF(AND (Account__c != NULL, Opportunity__c != NULL, Order__c != NULL), &apos;Account, Opportunity, Order&apos;, 
IF(AND (Account__c = NULL, Opportunity__c != NULL, Order__c = NULL), &apos;Opportunity&apos;, 
IF(AND (Account__c = NULL, Opportunity__c != NULL, Order__c != NULL), &apos;Opportunity, Order&apos;,
IF(AND (Account__c = NULL, Opportunity__c = NULL, Order__c != NULL), &apos;Order&apos;, &apos;&apos;
))))))</formula>
        <label>Linked Objects</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Opportunity_Account__c</fullName>
        <description>Account Id tied to Opportunity</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(Opportunity__r.AccountId)</formula>
        <label>Opportunity Account</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Opportunity to which the file is associated</description>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Content Metadata</relationshipLabel>
        <relationshipName>Content_Metadata</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Order_Account__c</fullName>
        <description>Account tied to the Order</description>
        <externalId>false</externalId>
        <formula>CASESAFEID(Order__r.AccountId)</formula>
        <label>Order Account</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Order to which the file is associated</description>
        <externalId>false</externalId>
        <label>Order</label>
        <referenceTo>Order</referenceTo>
        <relationshipLabel>Content Metadata</relationshipLabel>
        <relationshipName>Content_Metadata</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Owned_By_User__c</fullName>
        <externalId>false</externalId>
        <formula>Owner:User.FirstName + &apos; &apos; + Owner:User.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Owned By User</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Source__c</fullName>
        <description>Records the document source</description>
        <externalId>false</externalId>
        <label>Source</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Document_Source</valueSetName>
        </valueSet>
    </fields>
    <label>Content Metadata</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Document_Type__c</columns>
        <columns>Content_Size__c</columns>
        <columns>Source__c</columns>
        <columns>Owned_By_User__c</columns>
        <columns>Document_Created_By__c</columns>
        <columns>Document_Created_Date__c</columns>
        <columns>Account__c</columns>
        <columns>Opportunity__c</columns>
        <columns>Order__c</columns>
        <columns>Linked_Objects__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <encryptionScheme>None</encryptionScheme>
        <label>Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Content Metadata</pluralLabel>
