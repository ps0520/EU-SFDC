    <validationRules>
        <fullName>Addresses_4_CH_AT_Shd_Start_With_Alphabe</fullName>
        <active>true</active>
        <description>Addresses for CH and AT are incorrectly imported into Salesforce from Google Maps API. The Number should follow the street name.</description>
        <errorConditionFormula>AND(OR( TEXT(Country__c) = &quot;Switzerland&quot;, TEXT(Country__c) = &quot;Austria&quot;),
REGEX( Address_Line_1__c , &quot;\\d+?.*&quot;))</errorConditionFormula>
        <errorMessage>Address should not start with a number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Address_Country_Selection_Chk</fullName>
        <active>true</active>
        <description>Chosen address country should match the default Country selection on the Account record.</description>
        <errorConditionFormula>AND( 
ISCHANGED(Country__c),
OR(
ISBLANK(Text(Country__c)),
Text(Account__r.Country__c) != Text(Country__c)
)
)</errorConditionFormula>
        <errorMessage>Address country does not match with the Account country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Address_Fields_Are_Required</fullName>
        <active>true</active>
        <description>Rule to verify if all address fields are filled in when saving address record</description>
        <errorConditionFormula>OR( ISBLANK(Address_Line_1__c ) 
, ISBLANK( City__c ),ISBLANK( Postal_Code__c ),ISBLANK( TEXT(Country__c) ))</errorConditionFormula>
        <errorMessage>Address Line 1, City,  Postal Code and Country are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Address_Type_Cannot_Be_Changed</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Type__c )</errorConditionFormula>
        <errorMessage>Address Type cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Address_Type_Is_Required</fullName>
        <active>true</active>
        <description>Address Type is required on Address creation</description>
        <errorConditionFormula>ISBLANK(TEXT(Type__c))</errorConditionFormula>
        <errorMessage>Type is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>All_Locked_Addresses_Not_Updateable</fullName>
        <active>true</active>
        <description>If an Address is Locked, it should not be updated.</description>
        <errorConditionFormula>AND(  IsLocked__c, 
NOT(OR(
$Profile.Name = &quot;System Administrator&quot;,
$Profile.Name = &quot;Data Integrator&quot;
)),
OR(
ISCHANGED(Account__c),  ISCHANGED(Address_Line_1__c),  ISCHANGED(Address_Line_2__c),  ISCHANGED(Address_Line_3__c) ,  ISCHANGED(Business_Name__c),  ISCHANGED(City__c),  ISCHANGED(Country__c),  ISCHANGED(First_Name__c),  ISCHANGED(Last_Name__c),  ISCHANGED(Postal_Code__c),  ISCHANGED(Primary_Phone__c),  ISCHANGED(RecordTypeId),  ISCHANGED(State__c),  ISCHANGED(Type__c)))</errorConditionFormula>
        <errorMessage>Locked Address can&apos;t be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_PostalCode_Validation</fullName>
        <active>true</active>
        <description>Postal Code should follow the pattern.</description>
        <errorConditionFormula>AND(NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
BEGINS(RecordType.DeveloperName, &apos;CA&apos;),
NOT(ISBLANK(  Postal_Code__c)),
NOT(REGEX(Postal_Code__c, &apos;[A-Z][0-9][A-Z] [0-9][A-Z][0-9]&apos;)))</errorConditionFormula>
        <errorDisplayField>Postal_Code__c</errorDisplayField>
        <errorMessage>Invalid Postal Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_Province_Validation</fullName>
        <active>true</active>
        <description>Province is required.</description>
        <errorConditionFormula>AND(
    NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
    BEGINS(RecordType.DeveloperName, &apos;CA&apos;),
    ISBLANK(Text(State__c))
)</errorConditionFormula>
        <errorMessage>Province is a required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_Street_Should_Not_B_More_Than_30_Chr</fullName>
        <active>false</active>
        <description>Address lines should not be more than 30 characters in length</description>
        <errorConditionFormula>OR
(
	AND(  
		NOT(ISBLANK(Address_Line_1__c)),
		TEXT(Country__c) = &apos;Canada&apos;, 
		LEN(Address_Line_1__c) &gt; 30
	),
	AND(
		NOT(ISBLANK(Address_Line_2__c)),
		TEXT(Country__c) = &apos;Canada&apos;, 
		LEN(Address_Line_2__c) &gt; 30
	),
	AND(
		NOT(ISBLANK(Address_Line_3__c)),
		TEXT(Country__c) = &apos;Canada&apos;, 
		LEN(Address_Line_3__c) &gt; 30
	)
)</errorConditionFormula>
        <errorMessage>Address lines should not be more than 30 characters in length.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DE_Zip_Should_Be_5_Chars_In_Length</fullName>
        <active>true</active>
        <description>Zip cannot be more or less than 5 characters in Length for Germany</description>
        <errorConditionFormula>AND(
	TEXT(Country__c) = &apos;Germany&apos;, 
	OR (LEN(Postal_Code__c) &gt; 5, LEN(Postal_Code__c) &lt; 5)  
)</errorConditionFormula>
        <errorMessage>Postal Code should be 5 chars in length.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SI_Zip_Should_Be_4_Chars_In_Length</fullName>
        <active>true</active>
        <description>Zip cannot be more or less than 4 characters in Length for Slovenia</description>
        <errorConditionFormula>AND(
	TEXT(Country__c) = &apos;Slovenia&apos;, 
	OR (LEN(Postal_Code__c) &gt; 4, LEN(Postal_Code__c) &lt; 4)  
)</errorConditionFormula>
        <errorMessage>Postal Code should be 4 chars in length.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
