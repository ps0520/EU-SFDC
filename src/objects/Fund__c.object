        <fullName>Is_Fund_Active__c</fullName>
        <description>Check if the current date falls between the Auth start and end date.</description>
        <externalId>false</externalId>
        <formula>If(ISBLANK(Auth_End_Date__c), TRUE, If(Today() &lt;=  Auth_End_Date__c, TRUE, FALSE))</formula>
