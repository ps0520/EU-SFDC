    <validationRules>
        <fullName>Auth_End_Date_Greater_Than_Start_Date</fullName>
        <active>false</active>
        <description>Rule to check if the auth end date should be greater than auth start date</description>
        <errorConditionFormula>Auth_Start_Date__c &gt;  Auth_End_Date__c</errorConditionFormula>
