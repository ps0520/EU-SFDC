        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Primary_Shipping_Address__c</fullName>
        <externalId>false</externalId>
        <formula>Account__r.ShippingStreet + &apos;, &apos; +  Account__r.ShippingCity + &apos;, &apos; +  Account__r.ShippingPostalCode + &apos;, &apos; +  Account__r.ShippingCountry</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Primary Shipping Address</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
