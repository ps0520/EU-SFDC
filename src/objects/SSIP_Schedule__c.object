        </valueSet>
    </fields>
    <fields>
        <fullName>Primary_Billing_Address__c</fullName>
        <externalId>false</externalId>
        <formula>Account__r.BillingStreet + &apos;, &apos; + Account__r.BillingCity + &apos;, &apos; + Account__r.BillingPostalCode + &apos;, &apos; + Account__r.BillingCountry</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
