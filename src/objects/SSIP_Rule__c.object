    <validationRules>
        <fullName>Check_Account_Fund_For_Funded_Rule</fullName>
        <active>true</active>
        <description>If Fund is selected for the Rule, check if the account has Fund associated.</description>
        <errorConditionFormula>AND(ISPICKVAL( Is_Funded__c, &apos;True&apos; ), ISBLANK( Account__r.Fund__c ))</errorConditionFormula>
        <errorMessage>Account does not have Fund association, please associate a Fund before enabling the Rule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Primary_CC_Exist_For_CC_Pay_Term</fullName>
        <active>true</active>
        <description>If Payment Term is CC then the Primary CC should be associated to the customer</description>
        <errorConditionFormula>AND(
    NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
    ISPICKVAL(Payment_Term__c, &apos;net0&apos;),  
    ISBLANK(Account__r.Primary_Credit_Card__c) 
)</errorConditionFormula>
        <errorMessage>Account does not have any Primary Credit Card associated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Be_Greater_Than_The_Start_Date</fullName>
        <active>true</active>
        <description>SSIP Rule End Date or Renewal date should be greater than the Rule Start Date</description>
        <errorConditionFormula>OR(Rule_End_Date__c &lt; Rule_Start_Date__c,
 Renewal_Date__c &lt; Rule_Start_Date__c)</errorConditionFormula>
        <errorMessage>&apos;Rule End Date&apos; or &apos;Renewal Date&apos; must be greater than the &apos;Rule Start Date&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Can_Not_Be_Less_Than_Today</fullName>
        <active>false</active>
        <description>If End Date or Renewal date changed then cannot select prior date than today</description>
        <errorConditionFormula>OR(Rule_End_Date__c &lt; TODAY(),
 Renewal_Date__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>&apos;Rule End Date&apos; or &apos;Renewal Date&apos; can&apos;t be less than Today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Shipment_Date_Between_Rule_Dates</fullName>
        <active>false</active>
        <description>First Shipment Date must fall between rule start date and rule end date</description>
        <errorConditionFormula>AND(
NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;), 
OR(
First_Shipment_Date__c &lt; Rule_Start_Date__c ,
AND(
NOT( ISBLANK( Rule_End_Date__c ) ) ,
First_Shipment_Date__c &gt; Rule_End_Date__c
))
)</errorConditionFormula>
        <errorMessage>&apos;First Shipment Date&apos; must fall between &apos;Rule Start Date&apos; and &apos;Rule End Date&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fund_Selection_Check</fullName>
        <active>true</active>
        <description>If the payment term is credit card or prepaid then the Fund is not applicable.</description>
        <errorConditionFormula>AND(
    NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
    ISCHANGED( Is_Funded__c ), 
    ISPICKVAL(Is_Funded__c, &apos;True&apos;), 
    OR(ISPICKVAL( Payment_Term__c, &apos;net0&apos;), ISPICKVAL( Payment_Term__c, &apos;net-1&apos;))
)</errorConditionFormula>
        <errorMessage>Rule cannot be funded if the payment term is either Prepaid or Credit Card.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Term_Check</fullName>
        <active>true</active>
        <description>Payment Term Credit Card can be chosen if the customer is paying. Not applicable for Funded or Payors.</description>
        <errorConditionFormula>AND(
NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
NOT(ISBLANK(Effective_Payor__c)), ISPICKVAL( Payment_Term__c, &apos;net0&apos;))</errorConditionFormula>
        <errorMessage>Payment Term &apos;Credit Card&apos; cannot be chosen as there is a Payor association to the Rule.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payor_Selection_Check</fullName>
        <active>true</active>
        <description>If the payment term is credit card or prepaid then the Payor cannot be added.</description>
        <errorConditionFormula>AND(
NOT($Profile.Name = &quot;System Administrator&quot;),NOT($User.Alias = &quot;autoproc&quot;),
OR(ISCHANGED(Payor_Exist__c), ISCHANGED(Is_Funded__c)),
OR(ISPICKVAL(Payor_Exist__c, &apos;True&apos;), ISPICKVAL(Is_Funded__c, &apos;True&apos;)),
OR(ISPICKVAL( Payment_Term__c, &apos;net0&apos;), ISPICKVAL( Payment_Term__c, &apos;net-1&apos;))
)</errorConditionFormula>
        <errorMessage>Payor cannot be selected if the payment term either Prepaid or Credit Card.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Change_Is_Not_Allowed</fullName>
        <active>true</active>
        <description>Product is not changeable. You should cancel SSIP Rule and create new one for new Product.</description>
        <errorConditionFormula>ISCHANGED(Product__c)</errorConditionFormula>
        <errorMessage>Product is not changeable. You should cancel SSIP Rule and create new one for new Product.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
